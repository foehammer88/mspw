<h1>Listing analytics</h1>

<table>
  <tr>
    <th>College</th>
    <th>Amount student used</th>
    <th>Amount money</th>
    <th>Total on campus</th>
    <th>Year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @analytics.each do |analytic| %>
  <tr>
    <td><%= analytic.college %></td>
    <td><%= analytic.amount_student_used %></td>
    <td><%= analytic.amount_money %></td>
    <td><%= analytic.total_on_campus %></td>
    <td><%= analytic.year %></td>
    <td><%= link_to 'Show', analytic %></td>
    <td><%= link_to 'Edit', edit_analytic_path(analytic) %></td>
    <td><%= link_to 'Destroy', analytic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Analytic', new_analytic_path %>
